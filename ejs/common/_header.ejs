<div class="cms-heading">
	<h2 class="cms-heading__ttl"><%= pageData.title %></h2>
	<ul class="cms-heading__control">
		<li class="cms-heading__control-notification"><a href="#"><img src="../img/common/icon-notification.png" alt="" width="40"></a></li>
		<li class="cms-heading__control-notification"><a href="#"><img src="../img/common/vn.png" alt="" width="24"></a></li>
		<li class="cms-heading__control-user">
			<a href="#" class="wp js-dropdown">
				<div class="cms-heading__control-avatar">
					<img src="../img/common/avatar.png" alt="">
				</div>
				<div class="cms-heading__control-info">
					<span class="namme">Quản Trị Viên</span>
					<span class="position">Super Admintrator</span>
				</div>
			</a>
			<ul class="c-dropdown">
